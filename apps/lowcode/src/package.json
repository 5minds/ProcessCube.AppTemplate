{
  "name": "@5minds/node-red-dashboard-2-myproject-package",
  "version": "0.0.1",
  "description": "A project spezfic node for node-red",
  "scripts": {
    "build:hello": "cross-env WIDGET_NAME=ui-hello ENTRY=ui/exports/ui-hello.js vite build",
    "build:thermo": "cross-env WIDGET_NAME=ui-thermo ENTRY=ui/exports/ui-thermo.js vite build",
    "build": "npm run build:hello && npm run build:thermo"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/5minds/ProcessCube.AppTemplate"
  },
  "keywords": [
    "node-red",
    "processcube",
    "k8s",
    "kubernetes",
    "contextstore",
    "runtime",
    "lowcode"
  ],
  "authors": [
    {
      "name": "Martin MÃ¶llenbeck",
      "email": "Martin.Moellenbeck@5Minds.de"
    }
  ],
  "license": "MIT",
  "node-red": {
    "nodes": {
      "sample_plugin": "nodes/aplugin/sample_plugin.js",
      "hello": "nodes/sample_node/hello.js",
      "ui-hello": "nodes/ui-hello.js",
      "ui-thermo": "nodes/ui-thermo.js"
    }
  },
  "node-red-dashboard-2": {
    "version": "1.0.0",
    "widgets": {
      "ui-hello": {
        "output": "ui-hello.umd.js",
        "component": "Hello"
      },
      "ui-thermo": {
        "output": "ui-thermo.umd.js",
        "component": "Thermo"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/5minds/ProcesssCube.AppTemplate/issues",
    "email": "processcube@5minds.de"
  },
  "homepage": "https://processcube.io/docs/node-red/processcube-app-template/",
  "dependencies": {
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "vite": "^5.4.0",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-static-copy": "^1.0.4",
    "cross-env": "^7.0.3",
    "vue": "^3.4.0",
    "vuex": "^4.1.0"
  }
}
